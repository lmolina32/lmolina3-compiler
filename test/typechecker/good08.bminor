// should pass
factorial: function integer (n: integer) = {
    a: auto;
    if (n <= 1) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}

main: function integer () = {
    nums: array [5] integer;
    
    i: integer; 
    for (i = 0; i < #nums; i++) {
        nums[i] = factorial(i + 1);
    }
    
    sum: integer = 0;
    j: integer; 
    for (j = 0; j < #nums; j++) {
        sum = sum + nums[j];
    }
    
    print "Sum of factorials: ", sum;
    return 0;
}