/* Simple integer arithmetic */
x: integer = 10;
y: integer = 20;

main: function integer () = {
    z: integer;
    z = x + y * 2; // Should be 50 (Order of operations)
    print "x: ", x, ", y: ", y, "\n";
    print "z (x + y * 2): ", z, "\n";
    
    z = z - 60; // Should be -10
    print "z - 60: ", z, "\n";
    
    return 0;
}