/* Control flow: If/Else and Loops */
main: function integer () = {
    i: integer;
    print "Counting 0 to 4:\n";
    for(i=0; i<5; i++) {
        print i, " ";
    }
    print "\n";

    t: boolean = true;
    if (t) {
        print "True logic works\n";
    } else {
        print "ERROR: True logic failed\n";
    }

    x: integer = 100;
    if (x > 50) {
        if (x < 150) {
            print "Nested If Works (100 is between 50 and 150)\n";
        }
    }
    return 0;
}